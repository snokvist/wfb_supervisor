# Command-driven config for wfb_supervisor (explicit cmd lines with substitutions)

[general]
init_cmd=/bin/sh -c 'kill $(pidof wfb_tun) $(pidof wfb_rx) $(pidof wfb_tx) 2>/dev/null || true'

[parameters]
master_node=192.168.2.20
link_id=7669206
mcs=2
ldpc=0
stbc=0
key_file=/etc/gs.key
log_interval=1000
restart=yes
restart_delay=3
REGION=US
CHANNEL=165
TXPOWER=500
# Bandwidth in MHz (helper scripts prepend HT as needed)
BANDWIDTH=20

[instance master-video-rx]
cpu=0
cmd=wfb_rx -a 5500 -K $key_file -c $master_node -u 5600 -R 2097152 -s 2097152 -l $log_interval -i $link_id
quiet=yes
